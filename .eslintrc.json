{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "react/prop-types": "off",
    "react/sort-comp": "off",
    "class-methods-use-this": "off"
  }
}